{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "GHS Multi Integration file",
	"patterns": [
		{
			"include": "#envVars"
		},
		{
			"include": "#comments"
		},
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		}
	],
	"repository": {
		"comments": {
			"patterns": [
				{
					"begin": "#",
					"end": "\\n",
					"name": "comment.line.number-sign.multi-int",
					"beginCaptures": {
						"0": {
							"name": "punctuation.definition.comment.multi-int"
						}
					}
				}
			]
		},
		"keywords": {
			"patterns": [
				{
					"name": "keyword.control.multi-int-file",
					"match": "%include\\b|\\b(AddressSpace|EndAddressSpace|EndObject|false|Filename|HeapSize|Language|Library|Link|MapTo|MaximumPriority|MeemoryRegion|Name|Object|OtherObjectName|Paged|Priority|StackLength|StartIt|true|UnmappedLibrary|Write)\\b"
				}
			]
		},
		"strings": {
			"name": "string.quoted.double.multi-int-file",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.multi-int-file",
					"match": "\\\\."
				},
				{
					"include": "#envVars"
				}
			]
		},
		"envVars": {
			"name": "variable.other.normal.multi-int",
			"begin": "\\$\\(",
			"end": "\\)"
		}
	},
	"scopeName": "source.multi-int"
}